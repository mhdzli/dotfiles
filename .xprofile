#!/usr/bin/sh

xset r rate 300 50 &	# Speed xrate up
unclutter &		# Remove mouse when idle
#xcompmgr &		# xcompmgr for transparency
compton &
dunst &	                # dunst for notifications
dwmblocks &             # dwmbar
sxhkd &
xrdb ~/.Xdefaults &
setbg &

setxkbmap -model asus_laptop -layout us,ir -option "grp:alt_shift_toggle,caps:escape_shifted_capslock,altwin:menu_win" &


