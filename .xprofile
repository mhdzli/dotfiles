#!/usr/bin/sh

xset r rate 300 50 &	# Speed xrate up
unclutter &		# Remove mouse when idle
xcompmgr &		# xcompmgr for transparency
dunst &	                # dunst for notifications
keymap &		# some keyboard remap 
dwmblocks &                # dwmbar
sxhkd &

#feh --randomize --bg-scale ~/Pictures/backgrounds
xwallpaper --stretch ~/Pictures/lockscreen.jpeg & # set wallpaper
setxkbmap -model asus_laptop -layout us,ir -option grp:lctrl_lwin_toggle &


